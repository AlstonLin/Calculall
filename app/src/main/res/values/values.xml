    <!-- THEME DAVID -->
    <style name="buttonDarkDavid" parent="android:Widget.Holo.Button.Borderless.Small">
        <item name="android:background">@drawable/button_dark_david</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="buttonLightDavid" parent="android:Widget.Holo.Button.Borderless.Small">
        <item name="android:background">@drawable/button_light_david</item>
